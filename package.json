{
  "name": "vs-code-code-suggest-and-explaining-extension",
  "displayName": "VS-Code-code-suggest-and-explaining-extension",
  "description": "",
  "version": "0.0.1",
  "publisher": "AbdulkaderSafi",
  "author": {
    "name": "Safi",
    "email": "safi.abdulkader@gmail.com",
    "url": "https://abdulkadersafi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Abdulkader-Safi/VS-Code-code-suggest-and-explaining-extension.git"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ollamaExplainer.explainCode",
        "title": "Explain Code with Ollama"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ollama-explainer",
          "title": "Ollama Code Explainer",
          "icon": "$(hubot)"
        }
      ]
    },
    "views": {
      "ollama-explainer": [
        {
          "type": "webview",
          "id": "ollamaExplainer",
          "name": "Code Explainer",
          "icon": "$(hubot)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.18.12",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "ollama": "^0.6.0"
  }
}